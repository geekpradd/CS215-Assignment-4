function [bases, eigenvalues] = highest_dimensions(data, n)
    mean = sum(data,2)/size(data,2);
    data = data - mean;
    covariance = data*data'/size(data, 2);
    [V, D] = eig(covariance);
    
    [~, ind] = sort(diag(D), 'descend');
    
    dimensions = ind(1:n);
    VS = V(:, ind);
    bases = VS(:, 1: n);
    
end